{
  "name": "expressible",
  "version": "0.1.3",
  "description": "Train small, local text classifiers from labeled examples. No API keys, no cloud, no GPU.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "expressible": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/ui/static dist/ui/static",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": ["ml", "local", "training", "cli", "inference", "classification", "nlp", "text-classification", "offline", "on-premise"],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/expressibleai/expressible-cli.git"
  },
  "homepage": "https://expressible.ai",
  "bugs": {
    "url": "https://github.com/expressibleai/expressible-cli/issues"
  },
  "author": "Expressible AI, Inc.",
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@xenova/transformers": "^2.17.2",
    "commander": "^12.1.0",
    "express": "^4.21.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.3.7",
    "open": "^10.1.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "vitest": "^2.1.0"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
