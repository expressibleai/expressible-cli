{
  "name": "distill-cli",
  "version": "0.1.0",
  "description": "Train small, local ML models from input/output example pairs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "distill": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/ui/static dist/ui/static",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": ["ml", "local", "training", "cli", "inference"],
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@xenova/transformers": "^2.17.2",
    "commander": "^12.1.0",
    "express": "^4.21.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.3.7",
    "open": "^10.1.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
